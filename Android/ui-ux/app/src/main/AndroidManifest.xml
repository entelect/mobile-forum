<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="za.co.entelect.kindling">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity
            android:name=".transition.TransitionExampleSecondScreenActivity"
            android:theme="@style/AppTheme.Custom" />
        <activity android:name=".transition.TransitionExampleActivity" />
        <activity android:name=".animate.AnimateExampleActivity" />
        <activity android:name=".sharedelement.SharedElementExampleActivity" />
        <activity android:name=".motionlayout.MotionLayoutExampleActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>